                    <Image Source="{Binding TokensName[0]}" Visibility="{Binding TokensVisibility[0]}" ToolTip="{Binding TokensHints[0]}">
                        <Image.InputBindings>
                            <MouseBinding Gesture="LeftClick" Command="{Binding SelectTokenCommand}" CommandParameter="0"/>
                        </Image.InputBindings>
                    </Image>
                    <Image Source="{Binding TokensName[1]}" Visibility="{Binding TokensVisibility[1]}" ToolTip="{Binding TokensHints[1]}">
                        <Image.InputBindings>
                            <MouseBinding Gesture="LeftClick" Command="{Binding SelectTokenCommand}" CommandParameter="1"/>
                        </Image.InputBindings>
                    </Image>
                    <Image Source="{Binding TokensName[2]}" Visibility="{Binding TokensVisibility[2]}" ToolTip="{Binding TokensHints[2]}">
                        <Image.InputBindings>
                            <MouseBinding Gesture="LeftClick" Command="{Binding SelectTokenCommand}" CommandParameter="2"/>
