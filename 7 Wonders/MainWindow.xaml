                    <Button Content="Око" FontSize="20" Command="{Binding PeekCommand}"/>
                </Border>
                <TextBox IsReadOnly="True" FontSize="32" TextAlignment="Center" Text="{Binding Help}" Foreground="{Binding HelpColor}"/>
