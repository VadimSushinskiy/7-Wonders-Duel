                        <TextBlock FontSize="44" Text="Жовті карти:" DockPanel.Dock="Left"/>
                        <TextBlock FontSize="44" Text="{Binding FirstPlayerWinPoints[2]}" DockPanel.Dock="Right"/>
                    </DockPanel>
