                </StackPanel>
            </Grid>
            <Canvas Width="700" Height="300" Visibility="{Binding MenuSettingVisibility[1], Converter={StaticResource BoolToVis}}">
                <Rectangle Fill="#ABBFAA" Width="700" Height="300" Opacity="0.9" RadiusX="35" RadiusY="35"/>
                <Button Content="Назад" FontSize="24" Width="200" Height="70" Command="{Binding ChangeMenuViewCommand}"/>
                <Label FontSize="32" Canvas.Top="125" Foreground="White" Canvas.Left="220">Вимкнути звук</Label>
                <CheckBox Canvas.Left="450" Canvas.Top="141" IsChecked="{Binding IsMuted}" Checked="CheckBox_Checked" Unchecked="CheckBox_Unchecked">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="2" ScaleY="2"/>
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Canvas>
        </Grid>
