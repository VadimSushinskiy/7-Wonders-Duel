                        <Button.Content>
                            <Image Source="../Images/Interface/menu-icon.png" Width="50"></Image>
                        </Button.Content>
                    </Button>
                </StackPanel>
                <TextBox Style="{StaticResource ResourceButtonTemplate}" DockPanel.Dock="Left" Width="125" BorderThickness="0" Text="{Binding War, StringFormat=War Points:&#x0d;&#x0a; {0}}"/>
                <TextBox Style="{StaticResource ResourceButtonTemplate}" DockPanel.Dock="Right" Width="125" BorderThickness="0" Text="{Binding War, StringFormat=War Points:&#x0d;&#x0a; {0}}"/>
