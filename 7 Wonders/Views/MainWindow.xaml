                    <Image Source="{Binding TokensName[0]}" Visibility="{Binding TokensVisibility[0], Converter={StaticResource BoolToVis}}" ToolTip="{Binding TokensHints[0]}">
                        <Image.InputBindings>
                            <MouseBinding Gesture="LeftClick" Command="{Binding SelectTokenCommand}" CommandParameter="0"/>
                        </Image.InputBindings>
                    </Image>
                    <Image Source="{Binding TokensName[1]}" Visibility="{Binding TokensVisibility[1], Converter={StaticResource BoolToVis}}" ToolTip="{Binding TokensHints[1]}">
                        <Image.InputBindings>
                            <MouseBinding Gesture="LeftClick" Command="{Binding SelectTokenCommand}" CommandParameter="1"/>
