        <Grid Grid.Row="0" Grid.RowSpan="2" Background="#3F000000" Visibility="{Binding MenuVisibility, Converter={StaticResource BoolToVis}}">
            <Grid Width="350" Height="450" Visibility="{Binding MenuSettingVisibility[0], Converter={StaticResource BoolToVis}}">
                <Rectangle Fill="#ABBFAA" Width="350" Height="450" Opacity="0.9" RadiusX="35" RadiusY="35"/>
