        <Grid Grid.Row="0" Grid.RowSpan="2" Width="1920" Height="1080" Background="AliceBlue" Visibility="{Binding EpochVisibility[3], Converter={StaticResource BoolToVis}}">
            <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
                <TextBlock FontSize="44" Foreground="{Binding WinColor}" FontWeight="Bold" Text="{Binding WinType}"/>
                <TextBlock FontSize="44" Foreground="{Binding WinColor}" FontWeight="Bold" Text="{Binding WinnerName, StringFormat=Переможець: {0}}"/>
                <Button Content="Реванш" Width="200" Height="70" Margin="0 0 0 20" Command="{Binding RestartCommand}"/>
                <Button Content="Вийти" Width="200" Height="70" Command="{Binding QiutCommand}"/>
            </StackPanel>
            <Canvas Visibility="{Binding StatisticVisibility, Converter={StaticResource BoolToVis}}">
                <StackPanel Canvas.Top="100" Canvas.Left="30">
                    <DockPanel>
                        <TextBlock FontSize="44" Text="{Binding FirstPlayerName}" DockPanel.Dock="Left"/>
                    </DockPanel>
                    <DockPanel LastChildFill="False">
                        <TextBlock FontSize="44" Text="Сині карти:" DockPanel.Dock="Left"/>
                        <TextBlock FontSize="44" Text="{Binding FirstPlayerWinPoints[0]}" DockPanel.Dock="Right"/>
                    </DockPanel>
                    <DockPanel LastChildFill="False">
                        <TextBlock FontSize="44" Text="Зелені карти:" DockPanel.Dock="Left"/>
